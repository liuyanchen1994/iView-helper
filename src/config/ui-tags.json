{
  "ButtonGroup": {
    "attributes": ["size", "shape","vertical"],
    "subTags":["Button"],
    "description": "基础组件，触发业务逻辑时使用。"
  },
  "Button": {
    "defaults": ["type"],
    "attributes": ["disabled", "ghost", "htmlType", "icon", "loading", "shape", "size", "type", "block"],
    "description": "基础组件，触发业务逻辑时使用。"
  },
  "Icon":{
    "attributes": ["type","size", "color","custom"],
    "description": "图标"
  },
  "Row": {
    "attributes": ["gutter", "type","align", "justify","class-name"],
    "subTags":["Col"],
    "description": "栅格栏中一行。"
  },
  "Col": {
    "defaults": [":span"],
    "attributes": ["span", "order","offset", "push","pull", "class-name","xs", "sm","md", "lg","xl", "xxl"],
    "description": "栅格栏中一列。"
  },
  "Layout": {
    "subTags":["Header","Sider","Content","Footer","Layout"],
    "description": "协助进行页面级整体布局。"
  },
  "Header": {
    "description": "布局容器。"
  },
  "Sider": {
    "attributes": ["breakpoint", "value","width", "collapsible","collapsed-width", "hide-trigger",
      "default-collapsed", "reverse-arrow","@on-collapse", "trigger","@toggleCollapse"],
    "description": "布局容器。"
  },
  "Content": {
    "description": "布局容器。"
  },
  "Footer": {
    "description": "布局容器。"
  },
  "List": {
    "attributes":["border","item-layout","header","footer","loading","size","split"],
    "subTags":["ListItem"],
    "description": "最基础的列表展示，可承载文字、列表、图片、段落，常用于后台数据展示页面。"
  },
  "ListItem": {
    "attributes":["action","extra"],
    "subTags":["ListItemMeta"],
    "description": "List Item"
  },
  "ListItemMeta": {
    "attributes":["avatar","title","description"],
    "description": "List Item Meta"
  },
  "Card":{
    "attributes":["bordered","dis-hover","shadow","padding","title","icon","to","replace","target","append","extra"],
    "description": "基础容器，用来显示文字、列表、图文等内容，也可以配合其它组件一起使用。"
  },
  "Collapse":{
    "attributes":["value","accordion","simple","@on-change"],
    "subTags":["Panel"],
    "description": "将内容区域折叠/展开。"
  },
  "Panel":{
    "attributes":["name","hide-arrow","content"],
    "description": "Panel"
  },
  "Split":{
    "attributes":["value","mode","min","max","@on-move-start","@on-moving","@on-move-end",
      "left","right","top","bottom","trigger"],
    "description": "可将一片区域，分割为可以拖拽调整宽度或高度的两部分区域。"
  },
  "Divider":{
    "attributes":["type","orientation","dashed","size"],
    "description": "分割线"
  },
  "CellGroup":{
    "attributes":["@on-click"],
    "subTags":["Cell"],
    "description": "单元格组"
  },
  "Cell":{
    "attributes":["name","title","label","extra","disabled","selected",
    "to","replace","target","append"],
    "description": "单元格"
  },
  "Menu":{
    "subTags":["MenuItem","Submenu","MenuGroup"],
    "attributes":["mode","theme","active-name","open-names","accordion","width",
    "@on-select","@on-open-change","@updateOpened","@updateActiveName"],
    "description": "导航菜单"
  },
  "MenuGroup":{
    "subTags":["MenuItem","Submenu"],
    "attributes":["title"],
    "description": "导航菜单组"
  },
  "MenuItem":{
    "attributes":["title"],
    "description": "导航菜单项"
  },
  "Submenu":{
    "attributes":["name"],
    "description": "导航菜单项"
  },
  "Tabs":{
    "subTags":["TabPane"],
    "attributes":["value","type","size","closable","animated","capture-focus",
    "before-remove","name","@on-click","@on-tab-remove","extra"],
    "description": "标签页"
  },
  "TabPane":{
    "attributes":["name","label","icon","disabled","closable","tab","index"],
    "description": "标签页子项"
  },
  "Dropdown":{
    "subTags":["DropdownItem"],
    "attributes":["trigger","visible","placement","transfer","transfer-class-name","stop-propagation",
    "@on-click","@on-visible-change","@on-clickoutside","list"],
    "description": "下拉菜单"
  },
  "DropdownItem":{
    "attributes":["name","disabled","divided","selected"],
    "description": "下拉菜单子项"
  },
  "Page":{
    "attributes":["current","total","page-size","page-size-opts","placement","size","simple",
      "show-total","show-elevator","show-sizer","class-name","styles","transfer","prev-text",
      "next-text","disabled","@on-change","@on-page-size-change"],
    "description": "分页"
  }
}

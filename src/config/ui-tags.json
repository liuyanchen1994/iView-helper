{
  "ButtonGroup": {
    "attributes": ["size", "shape","vertical"],
    "subTags":["Button"],
    "description": "基础组件，触发业务逻辑时使用。"
  },
  "Button": {
    "defaults": ["type"],
    "attributes": ["disabled", "ghost", "htmlType", "icon", "loading", "shape", "size", "type", "block"],
    "description": "基础组件，触发业务逻辑时使用。"
  },
  "Icon":{
    "attributes": ["type","size", "color","custom"],
    "description": "图标"
  },
  "Row": {
    "attributes": ["gutter", "type","align", "justify","class-name"],
    "subTags":["Col"],
    "description": "栅格栏中一行。"
  },
  "Col": {
    "defaults": [":span"],
    "attributes": ["span", "order","offset", "push","pull", "class-name","xs", "sm","md", "lg","xl", "xxl"],
    "description": "栅格栏中一列。"
  },
  "Layout": {
    "subTags":["Header","Sider","Content","Footer","Layout"],
    "description": "协助进行页面级整体布局。"
  },
  "Header": {
    "description": "布局容器。"
  },
  "Sider": {
    "attributes": ["breakpoint", "value","width", "collapsible","collapsed-width", "hide-trigger",
      "default-collapsed", "reverse-arrow","@on-collapse", "trigger","@toggleCollapse"],
    "description": "布局容器。"
  },
  "Content": {
    "description": "布局容器。"
  },
  "Footer": {
    "description": "布局容器。"
  },
  "List": {
    "attributes":["border","item-layout","header","footer","loading","size","split"],
    "subTags":["ListItem"],
    "description": "最基础的列表展示，可承载文字、列表、图片、段落，常用于后台数据展示页面。"
  },
  "ListItem": {
    "attributes":["action","extra"],
    "subTags":["ListItemMeta"],
    "description": "List Item"
  },
  "ListItemMeta": {
    "attributes":["avatar","title","description"],
    "description": "List Item Meta"
  },
  "Card":{
    "attributes":["bordered","dis-hover","shadow","padding","title","icon","to","replace","target","append","extra"],
    "description": "基础容器，用来显示文字、列表、图文等内容，也可以配合其它组件一起使用。"
  },
  "Collapse":{
    "attributes":["value","accordion","simple","@on-change"],
    "subTags":["Panel"],
    "description": "将内容区域折叠/展开。"
  },
  "Panel":{
    "attributes":["name","hide-arrow","content"],
    "description": "Panel"
  },
  "Split":{
    "attributes":["value","mode","min","max","@on-move-start","@on-moving","@on-move-end",

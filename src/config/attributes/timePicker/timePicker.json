{
  "TimePicker/type": {
    "options": [
      "time",
      "timerange"
    ],
    "description": "显示类型",
    "optionTyep": "String",
    "defaultValue": "time"
  },
  "TimePicker/value": {
    "description": "时间，可以是 JavaScript 的 Date，例如 new Date()，也可以是标准的时间格式，点击右边查看注意：value 使用 v-model 时，值是 Date 类型，可以配合 @on-change 使用",
    "optionTyep": "Date",
    "defaultValue": "time"
  },
  "TimePicker/format": {
    "description": "展示的时间格式",
    "optionTyep": "Date",
    "defaultValue": "HH:mm:ss"
  },
  "TimePicker/steps": {
    "description": "下拉列表的时间间隔，数组的三项分别对应小时、分钟、秒。例如设置为 [1, 15] 时，分钟会显示：00、15、30、45。",
    "optionTyep": "Array",
    "defaultValue": "[]"
  },
  "TimePicker/placement": {
    "options": [
      "top",
      "top-start",
      "top-end",
      "bottom",
      "bottom-start",
      "bottom-end",
      "left",
      "left-start",
      "left-end",
      "right",
      "right-start",
      "right-end"
    ],
    "description": "时间选择器出现的位置",
    "optionTyep": "String",
    "defaultValue": "bottom-start"
  },
  "TimePicker/placeholder": {
    "description": "占位文本",
    "optionTyep": "String",
    "defaultValue": "-"
  },
  "TimePicker/confirm": {
    "description": "是否显示底部控制栏",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },
  "TimePicker/open": {
    "description": "手动控制时间选择器的显示状态，true 为显示，false 为收起。使用该属性后，选择器不会主动关闭。建议配合 slot 及 confirm 和相关事件一起使用",
    "optionTyep": "Boolean",
    "defaultValue": "null"
  },
  "TimePicker/size": {
    "options": [
      "large",
      "small",
      "default"
    ],
    "description": "尺寸",
    "optionTyep": "String",
    "defaultValue": "-"
  },
  "TimePicker/disabled": {
    "description": "是否禁用选择器",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },
  "TimePicker/clearable": {
    "description": "是否显示清除按钮",
    "optionTyep": "Boolean",
    "defaultValue": "true"
  },
  "TimePicker/readonly": {
    "description": "完全只读，开启后不会弹出选择器，只在没有设置 open 属性下生效",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },
  "TimePicker/editable": {
    "description": "文本框是否可以输入，只在没有使用 slot 时有效",
    "optionTyep": "Boolean",
    "defaultValue": "true"
  },
  "TimePicker/transfer": {
    "description": "是否将弹层放置于 body 内，在 Tabs、带有 fixed 的 Table 列内使用时，建议添加此属性，它将不受父级样式影响，从而达到更好的效果 slot 时有效",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },
  "TimePicker/element-id": {
    "description": "给表单元素设置 id，详见 Form 用法。",
    "optionTyep": "String",
    "defaultValue": "-"
  },
  "TimePicker/separator": {
    "description": "两个日期间的分隔符",
    "optionTyep": "String",
    "defaultValue": "-"
  },
  "TimePicker/capture": {
    "description": "是否开启 capture 模式，也可通过全局配置",
    "optionTyep": "Boolean",
    "defaultValue": "true"
  },
  "TimePicker/@on-change": {
    "description": "时间发生变化时触发，返回值已经格式化后的时间，比如 09:41:00",
    "optionTyep": "function"
  },
  "TimePicker/@on-open-change": {
    "description": "弹出浮层和关闭浮层时触发，返回值true | false",
    "optionTyep": "function"
  },
  "TimePicker/@on-ok": {
    "description": "点击确定按钮时触发",
    "optionTyep": "function"
  },
  "TimePicker/@on-clear": {
    "description": "在清空日期时触发",
    "optionTyep": "function"
  }
}
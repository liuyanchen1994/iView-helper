{
  "Poptip/trigger": {
    "options":["hover","click","focus"],
    "description": "触发方式",
    "optionTyep": "String",
    "defaultValue": "click"
  },"Poptip/title": {
    "description": "显示的标题",
    "optionTyep": "String | Number | Slot",
    "defaultValue": "-"
  },"Poptip/content": {
    "description": "显示的正文内容，只在非 confirm 模式下有效",
    "optionTyep": "String | Number | Slot",
    "defaultValue": "-"

    "optionTyep": "String",
    "defaultValue": "bottom"
  },"Poptip/width": {
    "description": "宽度，最小宽度为 150px，在 confirm 模式下，默认最大宽度为 300px",
    "optionTyep": "String | Number",
    "defaultValue": "-"
  },"Poptip/confirm": {
    "description": "是否开启对话框模式",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },"Poptip/disabled": {
    "description": "是否禁用",
    "optionTyep": "Boolean",
    "defaultValue": "false"
  },"Poptip/ok-text": {

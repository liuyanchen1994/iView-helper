{
  "Split/value":{
    "description": "面板位置，可以是 0~1 代表百分比，或具体数值的像素，可用 v-model 双向绑定",
    "optionTyep": "Number | String",
    "defaultValue": "0.5"
  },
  "Split/mode":{
    "options":["horizontal","vertical"],
    "description": "类型，可选值为 horizontal 或 vertical",
    "optionTyep": "String",
    "defaultValue": "horizontal"
  },
  "Split/min":{
    "description": "最小阈值",
    "optionTyep": "Number | String",
    "defaultValue": "40px"
  },
  "Split/max":{
    "description": "最大阈值",
    "optionTyep": "Number | String",
    "defaultValue": "40px"
  },
  "Split/@on-move-start":{
    "description": "拖拽开始",
    "optionTyep": "Function"
  },
  "Split/@on-moving":{
    "description": "拖拽中",
    "optionTyep": "Function"
  },
  "Split/@on-move-end":{
    "description": "拖拽结束",
    "optionTyep": "Function"
  },
  "Split/left":{
    "description": "mode 为 horizontal 时可用，左边面板"
  },
  "Split/right":{
    "description": "mode 为 horizontal 时可用，右边面板"
  },
  "Split/top":{
    "description": "mode 为 vertical 时可用，上边面板"
  },
  "Split/bottom":{
    "description": "mode 为 vertical 时可用，下边面板"
  },
  "Split/trigger":{
    "description": "自定义分割拖拽节点"
  }
}